                                                        <TextBlock Text="Fixations until first click(count)"/>
                                                    </StackPanel>
                                                </CheckBox.ToolTip>
                                            </CheckBox>
                                            <CheckBox x:Name="cb_statistics_gaze_interaction2" Content="avg. gaze/mouse distance (px)" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,5,0,5" >
