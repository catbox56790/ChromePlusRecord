                                                    <StackPanel>
                                                        <TextBlock Text="Path length / The number of saccades(px)"/>
                                                    </StackPanel>
                                                </CheckBox.ToolTip>
                                            </CheckBox>
                                            <CheckBox x:Name="cb_statistics_mouse_saccade2" Content="avg. saccade duration (ms)" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,5,0,5" >
